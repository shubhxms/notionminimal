{
  "name": "nextjs-notion-starter-kit",
  "version": "0.1.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "posttest": "run-s build"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.4.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "fathom-client": "^3.5.0",
    "got": "^12.5.3",
    "install-peers": "^1.0.4",
    "isomorphic-unfetch": "^4.0.2",
    "lqip-modern": "^2.0.0",
    "next": "^13.1.2",
    "node-fetch": "^3.3.0",
    "notion-client": "^6.15.6",
    "notion-types": "^6.15.6",
    "notion-utils": "^6.15.6",
    "p-map": "^5.5.0",
    "p-memoize": "^7.1.1",
    "react": "^18.2.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-notion-x": "^6.15.7",
    "react-static-tweets": "^2.0.0",
    "react-use": "^17.4.0",
    "static-tweets": "^2.0.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.7",
    "@next/bundle-analyzer": "^13.1.2",
    "@types/classnames": "^2.3.1",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "pnpm": {
    "overrides": {
      "sharp@<0.30.5": ">=0.30.5",
      "loader-utils@>=1.0.0 <1.4.2": ">=1.4.2",
      "loader-utils@<1.4.1": ">=1.4.1",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "next@>=10.0.0 <12.1.0": ">=12.1.0"
    }
  }
}
